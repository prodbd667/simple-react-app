{
    "compilerOptions": {
      "outDir": "./build/",
      "sourceMap": true,
      "noImplicitAny": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "module": "commonjs",
      "target": "es5",
      "jsx": "react",
      "typeRoots": ["node_modules/@types", "src/custom-types"]
    },
    "include": ["./src/**/*"],
    "exclude": ["node_modules"]
  }